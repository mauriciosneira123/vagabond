{
    "name": "dyrynda/vagabond",
    "description": "Eloquent without a home",
    "type": "project",
    "homepage": "https://github.com/michaeldyrynda/vagabond",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/spark": "~1.0", 
        "laravel/cashier": "~6.0" 
    },
    "require-dev": {
        "dyrynda/nomad": "^1.1"
    },
    "repositories": [
        {
        "type": "composer",
        "url": "https://spark-satis.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "bin-dir": "./"
    }
}
